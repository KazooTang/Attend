<p>
<% if user_signed_in? %>
  <%= link_to "Sign Out", destroy_user_session_path, method: :delete, class: 'btn btn-default' %>
<% else %>
  <%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook), class: 'btn btn-default' %>
<% end %>
</p>
<%= month_calendar table: {class: 'table'}, events: @showsups, @events do |date, showsup, events| %>
  <%= render 'welcome/day', date: date, showsup: showsup %>
<% end %>

<% @user.each do |u| %>
  <div class='userHead'>
    <%= image_tag u.image, alt: u.name, class: 'hid' %>
    <%= u.name %>
  </div>
<% end %>
